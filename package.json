{
  "name": "face-liveness-detector",
  "version": "1.0.1",
  "description": "Vue 3 face detector component using human.js & openCV.js",
  "type": "module",
  "scripts": {
    "setup": "npm run copy:human-models && npm run download:tensorflow-wasm",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "publish": "node publish.js",
    "publish:build": "npm run build && npm run publish",
    "type-check": "vue-tsc --noEmit",
    "copy:human-models": "node copy-human-models.js",
    "download:tensorflow-wasm": "node download-tensorflow-wasm.js"
  },
  "dependencies": {
    "@techstark/opencv-js": "4.12.0-release.1",
    "@vladmandic/human": "^3.3.0",
    "vue": "^3.4.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.0",
    "@vue/test-utils": "^2.4.0",
    "@vue/tsconfig": "^0.5.0",
    "ssh2-sftp-client": "^12.0.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vue-tsc": "^1.8.0"
  }
}
