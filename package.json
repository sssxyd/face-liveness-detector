{
  "type": "module",
  "name": "@sssxyd/face-liveness-detector",
  "version": "0.4.0-alpha.28",
  "description": "Pure JS/TS implementation of liveness face detection based on human.js and opencv.js",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    },
    "./config": {
      "types": "./dist/types/config.d.ts",
      "import": "./dist/config.esm.js",
      "require": "./dist/config.js"
    },
    "./types": {
      "types": "./dist/types/types.d.ts",
      "import": "./dist/types.esm.js",
      "require": "./dist/types.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "README.en.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "build:watch": "rollup -c rollup.config.js -w",
    "build:uniapp": "rollup -c uniapp/rollup.config.js",
    "package:uniapp": "node uniapp/build.cjs",
    "download-wasm:plugin": "node uniapp/download-wasm-plugin.cjs",
    "install:uniapp": "node uniapp/install-plugin.cjs",
    "dev": "npm run build:watch",
    "clean": "rm -rf dist types",
    "type-check": "tsc --noEmit",
    "verify-types": "node verify-types.js"
  },
  "keywords": [
    "face-detection",
    "liveness-detection",
    "face-recognition",
    "anti-spoofing",
    "typescript"
  ],
  "author": "xuyd <sssxyd@gmail.com>",
  "license": "MIT",
  "peerDependencies": {
    "@techstark/opencv-js": "^4.12.0-release.1",
    "@vladmandic/human": "^3.3.0"
  },
  "peerDependenciesMeta": {
    "@techstark/opencv-js": {
      "optional": false
    },
    "@vladmandic/human": {
      "optional": false
    }
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.28.5",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.5",
    "@techstark/opencv-js": "^4.12.0-release.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.8.0",
    "@vladmandic/human": "^3.3.0",
    "fs-extra": "^11.3.2",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "rollup": "^3.29.4",
    "ts-jest": "^29.4.5",
    "tslib": "^2.8.1",
    "typescript": "^5.2.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sssxyd/face-liveness-detector.git"
  },
  "bugs": {
    "url": "https://github.com/sssxyd/face-liveness-detector/issues"
  },
  "homepage": "https://github.com/sssxyd/face-liveness-detector#readme",
  "docs": {
    "en": "https://github.com/sssxyd/face-liveness-detector/blob/main/README.md",
    "zh": "https://github.com/sssxyd/face-liveness-detector/blob/main/README.zh-Hans.md"
  }
}
